{
  "name": "tiptap-ui",
  "version": "1.0.0",
  "description": "UI components and styles for building beautiful text editors with Tiptap and Tailwind",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:vite": "vite build",
